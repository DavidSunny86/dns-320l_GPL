prefix=/home/patrick/nas/DNS-345_GPL/libtorrent-rasterbar-0.15.4/../xinst
exec_prefix=${prefix}
bindir=${exec_prefix}/bin
libdir=${exec_prefix}/lib
datarootdir=${prefix}/share
datadir=${datarootdir}
